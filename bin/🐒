#!  /usr/bin/env node
// 'use strict'

const fs = require("fs");

let path = process.argv[2];
let consolà = console;

let source = fs.readFileSync(path, 'utf-8')
// consolà.log(source);

let m = source.match(/[^a-zA-Z =(]"/);
consolà.log(m);
consolà.log(m.index);
consolà.log(m[0]);
// consolà.log(m.input[m.index, 2]);
consolà.log(m.input.substring(m.index, 2));
consolà.log("---");
consolà.log(source.substring(81, 83));
consolà.log(source.substring(m.index-1, m.index+1));

let n = source.matchAll(/[^a-zA-Z =(]"/g);
consolà.log(n);

consolà.log("---");
// n.each((o) => {
for (let o of n) {
  // consolà.log(o);
  consolà.log(source.substring(o.index-1, o.index+1));
};

consolà.log("---");
let p = "console.log(🐒);".match(/[^a-zA-Z =(.]/);
consolà.log(p);
consolà.log(p.input.substring(p.index, p.index+2));

