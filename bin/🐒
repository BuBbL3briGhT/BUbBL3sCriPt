#!  /usr/bin/env node
// 'use strict'

const fs = require("fs");

let path = process.argv[2];
let consol = console;

let source = fs.readFileSync(path, 'utf-8')
// consol.log(source);

let m = source.match(/[^a-zA-Z =(]"/);
consol.log(m);
consol.log(m.index);
consol.log(m[0]);
// consol.log(m.input[m.index, 2]);
consol.log(m.input.substring(m.index, 2));
consol.log("---");
consol.log(source.substring(81, 83));
consol.log(source.substring(m.index-1, m.index+1));

let n = source.matchAll(/[^a-zA-Z =(]"/g);
consol.log(n);

consol.log("---");
// n.each((o) => {
for (let o of n) {
  // consol.log(o);
  consol.log(source.substring(o.index-1, o.index+1));
};

consol.log("---");
let p = "console.log(游);".match(/[^a-zA-Z =(.]/);
consol.log(p);
consol.log(p.input.substring(p.index, p.index+2));

