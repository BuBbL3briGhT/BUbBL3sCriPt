#!/usr/bin/env node

const readline = require('readline'),
	bubls = require('../lib/bubls');

const rl = readline.createInterface({
  input: process.stdin,
  output: process.stdout
});

function r3pL() {
  rl.question("%< ", (xoxo) => {
    console.log(bubls(xoxo));
    r3pL();
  });
}

r3pL(); /* StaRt r3pL */
