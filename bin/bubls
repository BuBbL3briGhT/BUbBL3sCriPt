#!/usr/bin/env node

const readline = require('readline'),
      Bubblescript = require('../lib/bubblescript');

const bubls = new Bubblescript()

const rl = readline.createInterface({
  input: process.stdin,
  output: process.stdout
});

function r3pL() {
  rl.question("%< ", (xoxo) => {
    console.log(bubls.eval(xoxo));
    r3pL();
  });
}

r3pL(); /* StaRt r3pL */
